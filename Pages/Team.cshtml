@page
@model TeamModel
@{
  ViewData["Title"] = "Team";
}

<div class="carousellTeam">
  <div id="TeamMembers" class="carousel slide" data-ride="carousel">
    <ol class="carousel-indicators">
      <li data-target="#TeamMembers" data-slide-to="0" class="active"></li>
      <li data-target="#TeamMembers" data-slide-to="1"></li>
      <li data-target="#TeamMembers" data-slide-to="2"></li>
      <li data-target="#TeamMembers" data-slide-to="3"></li>
      <li data-target="#TeamMembers" data-slide-to="4"></li>
    </ol>
    <div class="carousel-inner">
      <div class="carousel-item active">
        <img class="d-block w-100 img-responsive" src="~/images/Mid.jpg" alt="First slide">
          <div class="carousel-caption center">
            <a href="https://twitter.com/Rhobalasv2/">
              <div class="carousellTxt">
                Rhobalas
              </div>
            </a>
          </div>
        </div>
        <div class="carousel-item">
          <img class="d-block w-100 img-responsive " src="~/images/Adc.jpg" alt="Second slide">
            <div class="carousel-caption text-center">
              <a href="https://twitter.com/potitearaignee">
                <div class="carousellTxt">
                  Araignée
                </div>
              </a>
            </div>
          </div>
          <div class="carousel-item">
            <img class="d-block w-100 img-responsive" src="~/images/Jgl.jpg" alt="Third slide">
              <div class="carousel-caption text-center">
                <a href="https://twitter.com/renewal_son">
                  <div class="carousellTxt">
                    Taikumi
                  </div>
                </a>
              </div>
            </div>
            <div class="carousel-item">
              <img class="d-block w-100 img-responsive" src="~/images/Support.jpg" alt="Fourth slide">
                <div class="carousel-caption text-center">
                  <a href="https://twitter.com/WylferV1">
                    <div class="carousellTxt">
                      Wylfer
                    </div>
                  </a>
                </div>
              </div>
              <div class="carousel-item">
                <img class="d-block w-100 img-responsive" src="~/images/Top.jpg" alt="Fifth slide">
                  <div class="carousel-caption text-center">
                    <a href="https://twitter.com/OneForOneV">
                      <div class="carousellTxt">
                        OneforOne
                      </div>
                    </a>
                  </div>
                </div>
                <a class="carousel-control-prev" href="#TeamMembers" role="button" data-slide="prev">
                  <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                  <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#TeamMembers" role="button" data-slide="next">
                  <span class="carousel-control-next-icon" aria-hidden="true"></span>
                  <span class="sr-only">Next</span>
                </a>
              </div>
            </div>
          </div>
        </div>
        <div class="container team">
          <div class="row featurette">
            <div class="col-md-7 order-md-2">
              <h2 class="featurette-heading">
                Elyandra Esport
              </h2>
              <p class="lead">French Esport Organisation & Training Center promoting fair competitiveness and Diversity since 2019.</p>
              <hr>
                <a href="mailto:contact@elyandra-esport.com">Email: contact@elyandra-esport.com</a>
                 
                  <div class="ref">

                  <a href="https://twitter.com/ElyandraEsport">Twitter: Elyandra Esport</a>
                  </div>
                </div>
                <div class="col-md-5 order-md-1 text-center">
                  <img src="~/images/RhobiElyandra.png" alt="Elyandraimg"></div>
                </div>
              </div>
            </div>